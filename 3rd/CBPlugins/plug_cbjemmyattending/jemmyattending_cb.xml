<?xml version="1.0" encoding="UTF-8"?>
<cbinstall version="1.9" type="plugin" group="user">
	<name>CB JEM MyAttending</name>
	<author>JEM Community</author>
	<creationDate>January 2015</creationDate>
	<copyright>Copyright (C) 2013-2015 joomlaeventmanager.net</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>info@joomlaeventmanager.net</authorEmail>
	<authorUrl>http://www.joomlaeventmanager.net</authorUrl>
	<version>2.0.4</version>
	<description>This plugin will show events where the user is attending.</description>
	<files>
		<filename plugin="jemmyattending_cb">jemmyattending_cb.php</filename>
		<filename>index.html</filename>
		<filename>jemmyattending_cb.css</filename>
		<filename>language/english.php</filename>
		<filename>language/index.html</filename>
	</files>
	<params>
	</params>
	<tabs>
		<tab name="My Attending" description="Attended Events" class="jemmyattendingTab" fields="0" position="cb_tabmain" displaytype="tab">
		<params>
			<param name="event_startdate" type="radio" default="0" label="Start Date" description="Show/Hide the start date">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
			<param name="event_enddate" type="radio" default="0" label="End Date" description="Show/Hide the end date">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
			<param name="event_description" type="radio" default="0" label="Event description" description="Show/Hide the description">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
			<param name="event_venue" type="radio" default="0" label="Event venue" description="Show/Hide venue">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
		</params>
			<fields>
			</fields>
		</tab>
	</tabs>
	<install>
		<queries>
		</queries>
	</install>
	<uninstall>
	</uninstall>
</cbinstall>